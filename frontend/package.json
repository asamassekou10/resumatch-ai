{
  "name": "frontend",
  "version": "2.0.0",
  "private": true,
  "dependencies": {
    "@stripe/react-stripe-js": "^2.9.0",
    "@stripe/stripe-js": "^3.5.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@vercel/analytics": "^1.6.1",
    "axios": "^1.6.2",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.263.1",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-helmet-async": "^2.0.5",
    "react-router-dom": "^6.30.2",
    "react-scripts": "5.0.1",
    "recharts": "^3.2.1",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "npm run generate:sitemap && react-scripts build",
    "build:prod": "npm run generate:sitemap && REACT_APP_ENABLE_ANALYTICS=true react-scripts build",
    "postbuild": "node scripts/postbuild.js",
    "generate:sitemap": "node scripts/generate-sitemap.js",
    "generate:favicon": "node scripts/generate-favicon.js",
    "test": "react-scripts test --watchAll=false",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:e2e": "echo 'E2E tests not configured yet'",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,json,css,md}",
    "eject": "react-scripts eject"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^@vercel/analytics/react$": "<rootDir>/src/__mocks__/@vercel/analytics/react.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.23",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.5.6",
    "prettier": "^3.1.1",
    "react-snap": "^1.23.0",
    "tailwindcss": "^3.4.1",
    "to-ico": "^1.1.5"
  },
  "reactSnap": {
    "inlineCss": false,
    "minifyHtml": {
      "collapseWhitespace": true,
      "removeComments": true
    },
    "include": [
      "/",
      "/guest-analyze",
      "/pricing",
      "/help",
      "/help/terms",
      "/help/privacy",
      "/login",
      "/register",
      "/blog",
      "/resources/for-students",
      "/resume-for/software-engineer",
      "/resume-for/nursing-student",
      "/resume-for/marketing-manager",
      "/resume-for/project-manager",
      "/resume-for/data-analyst",
      "/resume-for/registered-nurse",
      "/resume-for/accountant",
      "/resume-for/teacher",
      "/resume-for/sales-representative",
      "/resume-for/human-resources",
      "/resume-for/graphic-designer",
      "/resume-for/financial-analyst",
      "/resume-for/operations-manager",
      "/resume-for/customer-service",
      "/resume-for/business-analyst",
      "/resume-for/mechanical-engineer",
      "/resume-for/social-worker",
      "/resume-for/web-developer",
      "/resume-for/pharmacist",
      "/resume-for/electrician",
      "/resume-for/chef",
      "/blog/how-to-beat-ats-2026",
      "/blog/why-not-getting-interviews",
      "/blog/resume-keywords-project-managers",
      "/blog/software-engineer-resume-hiring-managers",
      "/blog/agile-project-manager-resume-keywords",
      "/blog/it-project-manager-resume-keywords",
      "/blog/nursing-resume-tips-healthcare",
      "/blog/financial-analyst-resume-keywords",
      "/blog/sales-representative-resume-keywords",
      "/blog/graphic-designer-resume-keywords",
      "/blog/resume-format-guide-2026",
      "/blog/data-scientist-resume-keywords",
      "/blog/medical-assistant-resume-keywords",
      "/blog/accountant-resume-keywords-cpa",
      "/blog/account-executive-resume-saas"
    ],
    "skipThirdPartyRequests": true,
    "cacheAjaxRequests": false,
    "puppeteerArgs": [
      "--no-sandbox",
      "--disable-setuid-sandbox",
      "--disable-web-security",
      "--disable-features=IsolateOrigins,site-per-process"
    ],
    "waitFor": 1000,
    "crawl": false,
    "removeStyleTags": false,
    "removeScriptTags": false,
    "ignoreForPrerender": [
      "onError",
      "pageerror"
    ]
  }
}
